<pretty-config xmlns="http://ocpsoft.org/prettyfaces/3.3.3"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://ocpsoft.org/prettyfaces/3.3.3
               http://ocpsoft.org/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">

    <url-mapping id="index">
        <pattern value="/index" />
        <view-id value="/index.xhtml" />
    </url-mapping>

    <url-mapping id="userEditMain">
        <pattern value="/user/edit" />
        <view-id value="/user_edit.xhtml" />
    </url-mapping>
    
    <url-mapping id="userDelete">
        <pattern value="/user/delete/#{user}"></pattern>
        <view-id value="/user_delete.xhtml"/>
    </url-mapping>

    <url-mapping id="userEdit">
        <pattern value="/user/edit/#{user}"></pattern>
        <view-id value="/user_edit.xhtml"/>
    </url-mapping>

</pretty-config>